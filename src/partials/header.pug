header.main-header
  nav.main-header__menu
    div
      a(href="https://github.com/javiluli/my-online-cv", target="_blank", title="github.com/javiluli/my-online-cv")
        img(src="./static/content/svg/cv-logo.svg", alt="cv-logo").cv-logo
    .main-end#main-end
      .main-menu-toggle#main_menu_toggle
        span.span-menu-toggle#span_menu_toggle
    nav.main-menu#main_menu
      ul.list-main-menu
        .colum-list
          li.main_menu__item: a(href="https://javiluli.github.io/hover-animations-btn/", target="_blank").main_menu__item-link Animated  buttons
          li.main_menu__item: a(href="https://javiluli.github.io/custom-animated-checkbox/", target="_blank").main_menu__item-link Animated checkbox 
          li.main_menu__item: a(href="https://javiluli.github.io/icons-svg/", target="_blank").main_menu__item-link Icons SVG
          //- li.main_menu__item: a(href="/", target="_blank").main_menu__item-link Contact us
        .colum-list
          li.main_menu__item.wrapper-settings#settings_icon Ajustes
            img(src="./static/content/svg/settings.svg", alt="" ).settings-icon
          li.main_menu__item.settings
            ul.list-settings#list_settings
              div
                li.item-settings
                  p.text-settings Tema oscuro
                  label.main-end_label(for='switch')
                    input#switch.inpt-switch-theme(type='checkbox', name='theme')
                li.item-settings
                  p.text-settings Idiomas
                  .change-languaje#change-languaje
                    a(href="#"): img(src="./static/content/svg/spain.svg", alt="cv-logo").img-languaje
                    a(href="#"): img(src="./static/content/svg/english.svg", alt="cv-logo").img-languaje
                    a(href="#"): img(src="./static/content/svg/france.svg", alt="cv-logo").img-languaje
                    a(href="#"): img(src="./static/content/svg/japan.svg", alt="cv-logo").img-languaje
